#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/MineTranslateV3.5/api/node_modules/.pnpm/jest@30.2.0_@types+node@24.10.1_ts-node@10.9.2/node_modules/jest/bin/node_modules:/home/MineTranslateV3.5/api/node_modules/.pnpm/jest@30.2.0_@types+node@24.10.1_ts-node@10.9.2/node_modules/jest/node_modules:/home/MineTranslateV3.5/api/node_modules/.pnpm/jest@30.2.0_@types+node@24.10.1_ts-node@10.9.2/node_modules:/home/MineTranslateV3.5/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/MineTranslateV3.5/api/node_modules/.pnpm/jest@30.2.0_@types+node@24.10.1_ts-node@10.9.2/node_modules/jest/bin/node_modules:/home/MineTranslateV3.5/api/node_modules/.pnpm/jest@30.2.0_@types+node@24.10.1_ts-node@10.9.2/node_modules/jest/node_modules:/home/MineTranslateV3.5/api/node_modules/.pnpm/jest@30.2.0_@types+node@24.10.1_ts-node@10.9.2/node_modules:/home/MineTranslateV3.5/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest@30.2.0_@types+node@24.10.1_ts-node@10.9.2/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../jest@30.2.0_@types+node@24.10.1_ts-node@10.9.2/node_modules/jest/bin/jest.js" "$@"
fi
